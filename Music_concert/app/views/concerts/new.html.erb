<h1>Concerts#new</h1>
<p>Find me in app/views/concerts/new.html.erb</p>

<%= form_for @concert do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist%>
	<br>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br>
	<%= f.label :venue %>
	<%= f.text_field :venue %>
	<br>
	<%= f.label :date %>
	<%= f.datetime_local_field :date %>
	<br>
	<%= f.label :price %>
	<%= f.number_field :price %>
	<br>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<br>
	<%= f.submit "Save" %>
<% end %>


